\n                >\r\n              </p>\r\n            </div>\r\n            <p>\r\n              <input\r\n                autocomplete=\"false\"\r\n                tabindex=\"0\"\r\n                type=\"text\"\r\n                class=\"py-1 px-1 text-gray-900 outline-none block h-full w-full\"\r\n              />\r\n            </p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"grid lg:grid-cols-2 gap-6 mt-5\">\r\n          <div\r\n            v-for=\"select in selects\"\r\n            :key=\"select\"\r\n            class=\"border focus-within:border-blue-500 focus-within:text-blue-500 transition-all duration-500 relative rounded p-1\"\r\n          >\r\n            <div class=\"-mt-4 absolute tracking-wider px-1 uppercase text-xs\">\r\n              <p>\r\n                <label for=\"name\" class=\"bg-white text-gray-600 px-1\"\r\n                  >{{ select }} *</label\r\n                >\r\n              </p>\r\n            </div>\r\n            <p>\r\n              <select\r\n                name=\"\"\r\n                class=\"py-1 px-1 text-gray-900 outline-none block h-full w-full\"\r\n              >\r\n                <option value=\"\">---</option>\r\n                <option value=\"volvo\">Volvo</option>\r\n              </select>\r\n            </p>\r\n          </div>\r\n        </div>\r\n        <div class=\"border-t mt-6 pt-3\">\r\n          <button\r\n            @click=\"close()\"\r\n            class=\"rounded text-gray-100 px-3 py-1 bg-blue-500 hover:shadow-inner hover:bg-blue-700 transition-all duration-300\"\r\n          >\r\n            {{ action }}\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  \r\n  \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Swal from \"sweetalert2\";\r\nimport addMachine from \"./addMachine.vue\";\r\nexport default {\r\n  name: \"DataTable\",\r\n  components: {\r\n    addMachine,\r\n  },\r\n  data() {\r\n    return {\r\n      action: \"\",\r\n      show: false,\r\n      id: \"\",\r\n      border: \"border border-gray-500 p-1 text-sm\",\r\n      headers: [\r\n        { title: \"Machine\", class: \"\" },\r\n        { title: \"SN\", class: \"sm:table-cell hidden\" },\r\n        { title: \"Reference\", class: \"sm:table-cell hidden\" },\r\n        { title: \"Model\", class: \"\" },\r\n        { title: \"RAM\", class: \"\" },\r\n        { title: \"Stockage\", class: \"\" },\r\n        { title: \"Processeur\", class: \"\" },\r\n        { title: \"OS\", class: \"xl:table-cell hidden\" },\r\n        { title: \"actions\" },\r\n      ],\r\n      inputs:[\r\n        \"nom de la mchine\",\r\n        \"reference\",\r\n        \"nombre serial\",\r\n        \"stockage\",\r\n      ],\r\n      selects:[\r\n        \"model\",\"os\"\r\n      ]\r\n    };\r\n  },\r\n  methods: {\r\n    tableRowBg(el) {\r\n      if (el.assigned) {\r\n        return this.border + \" \" + \"bg-red-100\";\r\n      } else {\r\n        return this.border + \" \" + \"bg-green-100\";\r\n      }\r\n    },\r\n    edit(id) {\r\n      console.log(id);\r\n      this.action = \"Edit\";\r\n      this.show = true;\r\n    },\r\n    destroy(id) {\r\n      Swal.fire({\r\n        title: \"Are you sure?\",\r\n        text: \"You won't be able to revert this!\",\r\n        icon: \"warning\",\r\n        showCancelButton: true,\r\n        confirmButtonColor: \"#3085d6\",\r\n        cancelButtonColor: \"#d33\",\r\n        confirmButtonText: \"Yes, delete it!\",\r\n      }).then((result) => {\r\n        if (result.isConfirmed) {\r\n          console.log(id);\r\n          this.$store.dispatch(\"deleteMchine\", id);\r\n          Swal.fire(\"Deleted!\", \"This machine has been deleted.\", \"success\");\r\n        }\r\n      });\r\n    },\r\n    close(){\r\n      this.show= false\r\n    }\r\n  },\r\n  mounted() {\r\n    this.$store.dispatch(\"getMachines\");\r\n  },\r\n  computed: {\r\n    machines() {\r\n      return this.$store.state.machines.results;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.modal{\r\n  left:10%\r\n  right: ;\r\n}\r\n</style>\r\n"]}]}